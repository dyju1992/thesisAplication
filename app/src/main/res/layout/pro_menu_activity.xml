<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dyju.thesisapplication.RegisterActivity"
    android:background="@drawable/tlo"
    android:backgroundTint="#60FFFFFF"
    android:backgroundTintMode="src_over">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/welcomeText"
        android:textColor="#0a1117"
        android:textStyle="bold"
        android:textSize="17dp"
        android:textAlignment="center"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/showManipulators"
        android:layout_marginTop="50dp"
        android:text="Wcześniej wyliczone wartości"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/addNewManipulator"
        android:layout_marginTop="110dp"
        android:text="Dodaj nowy manipulator"/>



</RelativeLayout>
